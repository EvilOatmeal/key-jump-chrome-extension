{
  "name": "key-jump-browser-extension",
  "version": "5.1.0",
  "description": "Easier keyboard navigation. No mouse needed to click on links and stuff.",
  "author": "Kenneth Sundqvist <kenneth@kesu.se> (kesu.se)",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lint": "prettier --check \"**/*.{md,json,js,css,html}\" && eslint . && web-ext lint",
    "lint:fix": "prettier --write \"**/*.{md,json,js,css,html}\" && eslint --fix . && web-ext lint",
    "test": "npm run lint",
    "build": "web-ext build",
    "start": "web-ext run",
    "serve-tests": "node npm-scripts/start.js"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "eslint": "7.11.0",
    "eslint-config-prettier": "6.12.0",
    "prettier": "2.1.2",
    "web-ext": "5.2.0"
  }
}
